{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-curly-newline": 0,
    "react/jsx-wrap-multilines": 0,
    "global-require": 0
  }
}
